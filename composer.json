{
  "name": "artisanpack-ui/media-library",
  "description": "Media library package for ArtisanPack UI",
  "type": "library",
  "license": "GPL-3.0-or-later",
  "autoload": {
    "psr-4": {
      "ArtisanPackUI\\MediaLibrary\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Jacob Martella",
      "email": "me@jacobmartella.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "laravel/framework": "^12.0",
    "laravel/sanctum": "^4.1",
    "illuminate/support": "^12.17.0"
  },
  "require-dev": {
    "pestphp/pest": "^3.8",
    "pestphp/pest-plugin-laravel": "^3.1"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ArtisanPackUI\\MediaLibrary\\MediaLibraryServiceProvider"
      ],
      "aliases": {
        "MediaLibrary": "ArtisanPackUI\\MediaLibrary\\Facades\\MediaLibrary"
      }
    }
  }
}
